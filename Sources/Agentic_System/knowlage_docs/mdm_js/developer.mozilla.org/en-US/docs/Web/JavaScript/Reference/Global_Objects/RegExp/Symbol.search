    <!--lit-part J1rEYwaybRw=--><!--lit-node 0--><h2 id="try_it" class="heading"><!--lit-node 1--><a class="heading-anchor" href="#try_it">Try it</a></h2><!--/lit-part-->
    <interactive-example name="JavaScript Demo: RegExp.prototype[Symbol.search]()"></interactive-example>
<div class="code-example"><pre class="brush: js interactive-example notranslate"><code>class RegExp1 extends RegExp {
  constructor(str) {
    super(str);
    this.pattern = str;
  }
  [Symbol.search](str) {
    return str.indexOf(this.pattern);
  }
}

console.log("table football".search(new RegExp1("foo")));
// Expected output: 6
</code></pre></div>
  </section><section
